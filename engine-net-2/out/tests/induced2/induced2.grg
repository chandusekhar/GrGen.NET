rule init1
{

    replace
    {
        a:Node;
        b:Node;
        
        a -e:Edge-> b;
        a -f:Edge-> b;
        a -g:Edge-> b;
    }
}

rule init2
{

    replace
    {
        a:Node;
        b:Node;
        a --> b;
        a --> a;
    }
}

