rule init1
{

    replace
    {
        x:Node;
        y:Node;
        z:Node;
        
        x --> y;
        y --> z;
    }
}

rule init2
{

    replace
    {
        x:Node;
        y:Node;
        z:Node;
        
        x --> x;
        x --> y;
        y --> z;
    }
}

rule init3
{

    replace
