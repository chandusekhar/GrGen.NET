exact rule testHom
{
	x: Node;
	y: Node;
	
	hom(x,y);
	
	-e:Edge-> x -f:Edge-> y;
	
	modify
	{
		delete(y);
	}
}

exact rule withDot
{
	.-->.;
	modify {}
}