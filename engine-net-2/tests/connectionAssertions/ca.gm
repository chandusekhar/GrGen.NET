node class N;
node class NN;

edge class onetoone
	connect N --> N;

edge class tomany
	connect N[0:1] --> N[+];

edge class onetooneNN
	connect N --> NN;

edge class tomanyNN
	connect N[0:1] --> NN[+];
	
edge class onetooneI extends onetoone
	connect copy extends;

edge class tomanyI extends tomany
	connect copy extends;

edge class onetooneNNI extends onetooneNN
	connect copy extends;

edge class tomanyNNI extends tomanyNN
	connect copy extends;

edge class manytoone
	connect N[+] --> N;

arbitrary edge class onetooneA
	connect N ?--? NN;

arbitrary edge class tomanyA
	connect N[0:1] ?--? NN[+];

edge class onetooneAA extends onetooneA, Edge
	connect copy extends;

edge class tomanyAA extends tomanyA, Edge
	connect copy extends;

undirected edge class onetooneU
	connect N -- NN;

undirected edge class tomanyU
	connect N[0:1] -- NN[+];
