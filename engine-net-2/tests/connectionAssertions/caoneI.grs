new graph "connectionAssertions\ca"

new n1:N
new n1 -e1:onetooneI-> n1
validate
validate strict
new n1 -e2:onetooneI-> n1
validate
delete edge e1
delete edge e2
new n1 -e1:onetooneNNI-> n1
validate
validate strict

echo "The graph is FirstTestPassed"
clear graph

new n1:N
new n2:N
new n1 -e1:onetooneI-> n2
validate
validate strict
new n2 -e2:onetooneI-> n1
validate
validate strict
delete edge e1
delete edge e2
new n1 -e1:onetooneNNI-> n2
validate
validate strict
new n2 -e2:onetooneNNI-> n1
validate
validate strict

quit
