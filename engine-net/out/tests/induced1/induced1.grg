rule init1
{
    pattern
    {
    }
    replace
    {
        a:Node;
        b:Node;
        c:Node;
        d:Node;
        e:Node;
        a --> b;
        b --> c;
        c --> d;
        d --> e;
    }
}

rule makeUndirected
{
    induced pattern
    {
        a:Node -e:Edge-> b:Node;
    }
    replace
    {
        a -e-> b;
        b --> a;
    }
}